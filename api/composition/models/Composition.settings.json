      "type": "string"
    },
    "public": {
      "type": "boolean"
    },
    "lyrics": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "created": {
      "type": "date"
    },
    "lastEdited": {
      "type": "date"
    },
    "views": {
      "type": "biginteger"
    },
    "listens": {
      "type": "biginteger"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "vocals": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "instrumentals": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "audio": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "creator": {
      "model": "user",
      "plugin": "users-permissions"
    }
  }
